<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginPageFragment">
    <fragment
        android:id="@+id/loginPageFragment"
        android:name="com.example.recyclerviewexinclass.LoginPageFragment"
        android:label="LoginPageFragment"
        tools:layout="@layout/fragment_loginpage">
        <argument
            android:name="userName"
            app:argType="string" />
        <argument
            android:name="password"
            app:argType="string" />
        <action
            android:id="@+id/action_loginPageFragment_to_itemLayoutFragment"
            app:destination="@id/itemLayoutFragment" />
    </fragment>
    <fragment
        android:id="@+id/itemLayoutFragment"
        android:name="com.example.recyclerviewexinclass.ItemLayoutFragment"
        android:label="ItemLayoutFragment"
        tools:layout="@layout/item_layout"/>
</navigation>